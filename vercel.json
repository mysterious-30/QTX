{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/license/verify",
      "dest": "api/license.py"
    },
    {
      "src": "/premium/feature",
      "dest": "api/premium.py"
    }
  ]
}
